    "@playwright/test": "^1.31.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "allure-commandline": "^2.20.1",
    "allure-playwright": "^2.0.0-beta.24",
    "dot-env": "^0.0.1",
    "dotenv": "^16.0.1",
    "typescript": "^4.7.4"
  }
}